<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suppressions PUBLIC
	"-//Puppy Crawl//DTD Suppressions 1.1//EN"
	"http://www.puppycrawl.com/dtds/suppressions_1_1.dtd">
<suppressions>

	<!-- exclude auto-generated SCT ID service -->
	<suppress checks="."
		files="org[\\/]ihtsdo[\\/]sct[\\/]id[\\/]service" />

	<suppress checks="AvoidInlineConditionals"
		files="." />

	<suppress checks="FileTabCharacter"
		files="." />

	<suppress checks="LineLength"
		files="." />

	<suppress checks="MagicNumber"
		files=".*(?:Test)\.java$" />

	<suppress checks="PackageAnnotation"
		files="." />

	<suppress checks="VisibilityModifier"
		files="." />

</suppressions>